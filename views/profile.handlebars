<img src="{{profile.image_url}}" class="profile-image" >
<h1>{{profile.name}}, <span class="age">{{profile.age}}</span></h1>
<div class="breed">{{profile.breed}}</div>
<div class="profile">{{profile.profile}}</div>
<h2 class="messages-title">Comments</h2>
{{#each profile.comments}}
<div class="comments"><p>{{comment_text}}</p></div>
<hr>
{{/each}}
